{
  "name": "10x-astro-starter_v6",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "auto-format": "prettier --write . && eslint src --ext .ts,.tsx,.js,.jsx,.astro --fix --max-warnings=0 || true",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" playwright test --project=chromium --workers=1",
    "test:e2e:ui": "cross-env PLAYWRIGHT_UI_MODE=1 NODE_OPTIONS=\"--max-old-space-size=8192\" playwright test --ui --project=chromium",
    "test:e2e:debug": "cross-env PLAYWRIGHT_DEBUG_MODE=1 NODE_OPTIONS=\"--max-old-space-size=8192\" playwright test --debug --project=chromium",
    "test:e2e:setup": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" playwright test --project='setup db'",
    "test:e2e:direct": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" playwright test --project=direct-chromium --workers=1",
    "test:e2e:smoke": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" playwright test smoke-test.spec.ts --project=chromium --workers=1",
    "test:e2e:auth": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" playwright test auth-basic.spec.ts --project=chromium --workers=1",
    "test:e2e:flashcards": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" playwright test flashcards-basic.spec.ts --project=chromium --workers=1",
    "test:e2e:basic": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" playwright test smoke-test.spec.ts auth-basic.spec.ts flashcards-basic.spec.ts --project=chromium --workers=1",
    "clean:tmp": "node scripts/clean-tmp.js"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.9",
    "@astrojs/node": "^9.1.3",
    "@astrojs/react": "4.2.2",
    "@astrojs/sitemap": "3.3.0",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-slot": "^1.1.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/vite": "4.0.17",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "astro": "5.5.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "esbuild": "0.25.1",
    "lucide-react": "^0.487.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwind-merge": "^3.1.0",
    "tailwindcss": "4.0.17",
    "tw-animate-css": "^1.2.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.7",
    "@eslint/js": "9.23.0",
    "@playwright/test": "^1.43.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "4.2.5",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-aeaed83-20250323",
    "eslint-plugin-react-hooks": "5.2.0",
    "husky": "9.1.7",
    "jsdom": "^24.0.0",
    "lint-staged": "15.5.0",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.28.0",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "@testing-library/react": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    }
  }
}
